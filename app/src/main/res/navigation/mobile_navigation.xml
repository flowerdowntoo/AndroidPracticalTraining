    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.androidtraining2_08_1912120208.ui.home.HomeFragment"
        android:label="租车"

        tools:layout="@layout/fragment_home" >
        <!--跳转到python详情页-->
